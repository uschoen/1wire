<?xml version="1.0" encoding="UTF-8" ?>
<opt>
	<daemon>true</daemon>
	<intervall>120</intervall>
	<path>/usr/local/easyHC</path>
	<pidfile>/var/run/EHCgateway.pid</pidfile>
	<gateways>
		<onewire name="onewire temperatur sensor" package="Gateway" modul="onewire" typ="intervall" time="60">
		<config>
			<path>/sys/bus/w1/devices/w1_bus_master1/</path>
      		<tempDiv>0.5</tempDiv>
      		<temp_configfile>/usr/local/easyHC/conf/activ_device.xml</temp_configfile>
			<device_list>
				<N28-0115913432ff>
					<available>1</available>
					<device_id>28-0115913432ff</device_id>
					<ise_id>29680</ise_id>
					<device_name>test</device_name>
					<enable>1</enable>
					<timestamp>1461681823</timestamp>
					<value>0</value>
				</N28-0115913432ff>
			</device_list>
			</config>
		</onewire>
	</gateways>
	<connectors>
		<HomematicXML enable="false" name="Homematic Updater" package="Connector" modul="HomematicXML">
			<gateways>
      			<onewire></onewire>
			</gateways>
			<config>
				<hm_url>http://IP OFF HOMEMATIC/config/xmlapi/statechange.cgi</hm_url>
				<fields>
					<ise_id>ise_id</ise_id>
					<value>new_value</value>
				</fields>
			</config>
    	</HomematicXML>
 		<Mysql enable="false" name="MYSQL Statistik" package="Connector" modul="Mysql">
 		<gateways>
      		<onewire></onewire>
     	</gateways>
 		<config>
			<db_host>IP OFF MYSQL DATABASE</db_host>
      		<db_password>PASSWORD</db_password>
      		<db_name>DB NAME</db_name>
      		<db_table>TABLE</db_table>
      		<db_user>USER</db_user>
      		<fields>
        		<device_id>device</device_id>
        		<timestamp>timestamp</timestamp>
        		<value>value</value>
      		</fields>
		</config>
    	</Mysql>
	</connectors>
   <logging>
    <loglevel>
    <unkown>true</unkown>
    <name>output</name>
    </loglevel>
    <output>
    <console>
      <enable>true</enable>
      <loglevel>
        <dump>false</dump>
        <alert>true</alert>
        <critical>true</critical>
        <debug>true</debug>
        <emergency>true</emergency>
        <error>true</error>
        <info>true</info>
        <unkown>true</unkown>
      </loglevel>
    </console>
    <file>
      <clearlog>false</clearlog>
      <dir>/var/log/</dir>
      <enable>true</enable>
      <filename>EHC_error.log</filename>
      <filesize>10000</filesize>
      <holdzipfiles>3</holdzipfiles>
      <logrotation>true</logrotation>
      <loglevel>
        <info>false</info>
        <debug>false</debug>
        <dump>false</dump>
        <unkown>false</unkown>
      </loglevel>
    </file>
     <file>
      <clearlog>false</clearlog>
      <dir>/var/log/</dir>
      <enable>false</enable>
      <filename>EHC_info.log</filename>
      <filesize>10000</filesize>
      <holdzipfiles>3</holdzipfiles>
      <logrotation>true</logrotation>
      <loglevel>
        <alert>true</alert>
        <critical>true</critical>
        <debug>false</debug>
        <dump>false</dump>
        <emergency>true</emergency>
        <error>true</error>
        <info>true</info>
        <unkown>true</unkown>
      </loglevel>
    </file>
    <file>
      <clearlog>false</clearlog>
      <dir>/var/log/</dir>
      <enable>false</enable>
      <filename>EHC_dump.log</filename>
      <filesize>10000</filesize>
      <holdzipfiles>3</holdzipfiles>
      <logrotation>true</logrotation>
    </file>
    </output>
  </logging>
</opt>
