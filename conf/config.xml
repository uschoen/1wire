<?xml version="1.0" encoding="UTF-8"?>
<root>
	<daemon>false</daemon>
	<pidfile>/var/run/onewire.pid</pidfile>
	<path>/mnt/nas01/entwicklung/onewire/1wire</path>
	<onewire name="1 Wired Temperature" package="Raspberry" modul="wired1" enable="true">
	   	<config>
	   		<gpio_pin>4</gpio_pin>
			<intervall>30</intervall>
			<gpio_pin>4</gpio_pin>
			<tempDiv>0.5</tempDiv>
			<path>/sys/bus/w1/devices/w1_bus_master1/</path>
			<slaves>w1_master_slaves</slaves>
			<device_list>
			</device_list>
		</config>
	</onewire>
		<logging>
		<loglevel>
			<unkown>true</unkown>
		</loglevel>
		<output>
			<file>
				<enable>true</enable>
				<loglevel>
					<unkown>true</unkown>
					<error>true</error>
					<critical>true</critical>
					<alert>true</alert>
					<emergency>true</emergency>
					<debug>false</debug>
					<info>false</info>
				</loglevel>
				<filename>error.log</filename>
				<dir>/smnt/nas01/entwicklung/onewire/1wire/log/</dir>
				<clearlog>true</clearlog>
				<logrotation>true</logrotation>
				<holdzipfiles>3</holdzipfiles>
				<filesize>1000000</filesize>
			</file>
			<file>
				<enable>true</enable>
				<filename>debug.log</filename>
				<dir>/smnt/nas01/entwicklung/onewire/1wire/log/</dir>
				<clearlog>true</clearlog>
				<logrotation>true</logrotation>
				<holdzipfiles>3</holdzipfiles>
				<filesize>1000000</filesize>
			</file>
			<console>
				<enable>true</enable>
				<loglevel>
					<unkown>true</unkown>
					<error>true</error>
					<critical>true</critical>
					<alert>true</alert>
					<emergency>true</emergency>
					<debug>true</debug>
					<info>true</info>
				</loglevel>
			</console>
		</output>
	</logging>
</root>